{
    "name": "prismamedia/metrics",
    "description": "Prometheus metrics exporter",
    "keywords": ["metrics", "prometheus", "bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Prisma Media",
            "email": "opensource@prismamedia.com"
        },
        {
            "name": "Romain Jacquart",
            "email": "rjacquart@prismamedia.com"
        },
        {
            "name": "Jérôme Tamarelle",
            "email": "jerome@tamarelle.net"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "symfony/framework-bundle": "^3.1|^4.0",
        "symfony/phpunit-bridge": "^3.1|^4.0",
        "symfony/browser-kit": "^3.1|^4.0"
    },
    "autoload": {
        "psr-4": { "PrismaMedia\\Metrics\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "PrismaMedia\\Metrics\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
